public interface TeamService {
	public String createBmi(TeamBean team);
	public String avgHeight(TeamBean team);
	public String avgWeight(TeamBean team);
	public int maxHeight(TeamBean team);
	public int minHeight(TeamBean team);
	public void addList(TeamBean team);
	public TeamBean[] list();
	public void showlist(TeamBean team);
	public String maxName(TeamBean team);
	public String minName(TeamBean team);
	public String createGender(TeamBean team);
	public void create( TeamBean team);



	
}
